			"prettier-plugin-packagejson"
		],
		"printWidth": 80,
		"semi": true,
		"singleQuote": false,
		"tabWidth": 2,
		"trailingComma": "es5",
		"useTabs": true
	},
	"devDependencies": {
		"prettier": "^3.0.3",
		"prettier-plugin-packagejson": "^2.4.5",
		"typescript": "^5.2.2"
	}
