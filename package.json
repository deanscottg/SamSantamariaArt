	"main": "index.js",
	"scripts": {
		"build": "turbo build",
		"clean": "turbo clean",
		"dev": "turbo dev --no-cache  --continue",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"lint": "turbo lint",
		"test": "turbo test"
	},
	"prettier": {
		"bracketSpacing": true,
		"endOfLine": "lf",
